        <activity android:name=".activities.donateparicipation.DonateParticipationStateActivity">
        </activity>
        <activity android:name=".activities.donateparicipation.DonateParticipationActivity">
        </activity>
        <activity
                android:name=".activities.donate.DonatePaymentActivity"
                android:windowSoftInputMode="adjustResize">
        </activity>
        <activity android:name=".activities.donate.DonateDetailedActivity">
        </activity>
        <activity android:name=".activities.donate.DonateActivity">
        </activity>
        <activity android:name=".activities.berrycharge.BerryChargeActivity">
        </activity>
        <activity android:name=".activities.mypage.MyPageActivity">
        </activity>
        <activity android:name=".activities.home.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.login.BeginningActivity">
        </activity>
        <activity android:name=".activities.login.LoginActivity">
        </activity>
        <activity
                android:name=".activities.login.SignUpActivity"
                android:windowSoftInputMode="adjustResize">
        </activity>
        <activity android:name=".activities.deliveryreview.DeliveryReviewActivity">
        </activity>
        <activity android:name=".activities.berrycharge.DepositActivity">
        </activity>
        <activity android:name=".activities.berryusehistory.BerryHistoryActivity">
        </activity>
        <activity android:name=".activities.deliveryreview.BerryReviewActivity">
        </activity>
        <activity android:name=".activities.berrycharge.PaymentActivity">
        </activity>
        <activity android:name=".activities.stamp.StampActivity">
        </activity>
    </application>

</manifest>